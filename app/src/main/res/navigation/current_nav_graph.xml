<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/current_nav_graph"
    app:startDestination="@id/currentFragment">

    <fragment
        android:id="@+id/currentFragment"
        android:name="com.lasteyestudios.ipoalerts.tabs.current.CurrentFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_current">

        <action
            android:id="@+id/action_currentFragment_to_details_nav_graph"
            app:destination="@id/details_nav_graph">
            <argument
                android:name="searchId"
                app:argType="string" />
        </action>
    </fragment>
    <include app:graph="@navigation/details_nav_graph" />
</navigation>